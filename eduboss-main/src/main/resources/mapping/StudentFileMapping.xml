<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd"> 
    <mapping date-format="yyyy-MM-dd" map-null="false">
		<class-a>com.eduboss.domain.StudentFile</class-a>
		<class-b>com.eduboss.domainVo.StudentFileVo</class-b>
		<field>
			<a>student.id</a>
			<b>studentId</b>
		</field>
		<field>
			<a>student.name</a>
			<b>studentName</b>
		</field>	
		<field>
			<a>creator.userId</a>
			<b>creatorId</b>
		</field>
		<field>
			<a>creator.name</a>
			<b>creatorName</b>
		</field>			
		<field>
		   <a>studentFileType.value</a>
		   <b>studentFileTypeValue</b>
		</field>
		<field>
			<a>studentFileType.name</a>
			<b>studentFileTypeName</b>
		</field>
	</mapping>
</mappings>